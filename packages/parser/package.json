{
  "name": "@liesmich/parser",
  "version": "0.0.0",
  "description": "An express api router used to proxy requests to the upstream server",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "private": false,
  "keywords": [
    "api",
    "types"
  ],
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.js && npm run build:types",
    "build:types": "tsc --project ./tsconfig.json  -d --declarationDir dist/types --declarationMap --emitDeclarationOnly",
    "build:readme": "npx @appnest/readme generate --input ../package_readme_blueprint.md --config readme_config.json",
    "test": "mocha --config .mocharc.yml --experimental-specifier-resolution=node",
    "test:coverage": "c8 --config ../../.nycrc.json npm run test",
    "lint": "tslint -c tslint.json -p tsconfig.spec.json src/**/*.ts",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.spec.json src/**/*.ts",
    "docs": "typedoc --options ./typedoc.config.cjs",
    "prepublishOnly": "npm run test && npm run docs && npm run build",
    "version": "npm run build:readme && git add README.md"
  },
  "homepage": "https://liesmich.github.io/liesmich/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/liesmich/liesmich/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liesmich/liesmich.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "deepmerge": "~4.2.2",
    "front-matter": "^4.0.2",
    "markdown-it": "~12.2.0",
    "qs": "~6.10.1",
    "rehype-parse": "~8.0.2",
    "rehype-raw": "6.1.0",
    "rehype-stringify": "~9.0.2",
    "remark-gfm": "3.0.0",
    "remark-parse": "~10.0.0",
    "remark-stringify": "~10.0.1",
    "remark-toc": "~8.0.1",
    "unified": "10.1.0"
  },
  "devDependencies": {
    "@types/crypto-js": "4.0.2",
    "@types/deepmerge": "2.2.0",
    "@types/qs": "6.9.7",
    "@types/spark-md5": "3.0.2",
    "spark-md5": "3.0.2"
  }
}
